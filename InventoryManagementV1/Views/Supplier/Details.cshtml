
@model InventoryManagementV1.Models.Supplier
@{
    Layout = "~/Views/Shared/layout.cshtml";

}

<!DOCTYPE html>
<section class="content">
    <div class="row">
        <!-- left column -->
        <div class="col-md-8">
            <!-- general form elements -->
            <div class="box box-primary">
                <div class="box-header with-border text-center">
                    <h3 class="box-title">Supplier Details</h3>
                </div>
                <!-- /.box-header -->


                <div class="box-body">
                    <table class="table table-hover table-fixed">
                        <tbody>

                            <tr class="active">
                                <td class="text-primary text-right" width="30%">Company Name: </td>
                                <td style="font-weight:bold">@Model.Company_Name</td>
                            </tr>
                            <tr class="active">
                                <td class="text-primary text-right" width="30%">Company Address: </td>
                                <td style="font-weight:bold">@Model.Company_Address</td>
                            </tr>
                            <tr class="active">
                                <td class="text-primary text-right" width="30%">Company Mobile: </td>
                                <td style="font-weight:bold">@Model.Company_Mobile</td>
                            </tr>
                            <tr class="active">
                                <td class="text-primary text-right" width="30%">Contact Name: </td>
                                <td style="font-weight:bold">@Model.Contact_Name</td>
                            </tr>
                            <tr class="active">
                                <td class="text-primary text-right" width="30%">Contact Person Number: </td>
                                <td style="font-weight:bold">@Model.Contact_Person_Number</td>
                            </tr>
                            <tr class="active">
                                <td class="text-primary text-right" width="30%">Contact Person Address: </td>
                                <td style="font-weight:bold">@Model.Contact_Person_Address</td>
                            </tr>
                            <tr class="active">
                                <td class="text-primary text-right" width="30%">Email: </td>
                                <td style="font-weight:bold">@Model.Email</td>
                            </tr>
                            <tr class="active">
                                <td class="text-primary text-right" width="30%">Website: </td>
                                <td style="font-weight:bold">@Model.Website</td>
                            </tr>

                        </tbody>
                    </table>


                </div>

            </div>

            <!-- /.box -->

        </div>
        <!--/.col (left) -->

    </div>
    <!-- /.row -->
</section>


